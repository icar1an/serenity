{
  "host_permissions": [
    "https://*.youtube.com/*"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/LogoSerenity256px.png",
        "icons/IconSerenity256px.png",
        "icons/PlayerStartIconSerenity.svg",
        "icons/PlayerStopIconSerenity.svg",
        "icons/PlayerUploadIconSerenity.svg",
        "icons/PlayerUploadFailedIconSerenity.svg",
        "icons/PlayerCancelSegmentIconSerenity.svg",
        "icons/clipboard.svg",
        "icons/settings.svg",
        "icons/pencil.svg",
        "icons/check.svg",
        "icons/check-smaller.svg",
        "icons/upvote.png",
        "icons/downvote.png",
        "icons/thumbs_down.svg",
        "icons/thumbs_down_locked.svg",
        "icons/thumbs_up.svg",
        "icons/help.svg",
        "icons/report.png",
        "icons/close.png",
        "icons/skipIcon.svg",
        "icons/refresh.svg",
        "icons/beep.oga",
        "icons/pause.svg",
        "icons/stop.svg",
        "icons/skip.svg",
        "icons/heart.svg",
        "icons/visible.svg",
        "icons/not_visible.svg",
        "icons/sort.svg",
        "icons/money.svg",
        "icons/segway.png",
        "icons/close-smaller.svg",
        "icons/right-arrow.svg",
        "icons/campaign.svg",
        "icons/star.svg",
        "icons/lightbulb.svg",
        "icons/bolt.svg",
        "icons/stopwatch.svg",
        "icons/music-note.svg",
        "icons/import.svg",
        "icons/export.svg",
        "icons/PlayerInfoIconSerenity.svg",
        "icons/PlayerDeleteIconSerenity.svg",
        "icons/dearrow.svg",
        "icons/sb-pride.png",
        "icons/pride.svg",
        "popup.html",
        "popup.css",
        "content.css",
        "shared.css",
        "js/document.js",
        "libs/Source+Sans+Pro.css",
        "libs/6xKydSBYKcSV-LCoeQqfX1RYOo3ig4vwlxdu.woff2",
        "libs/6xKydSBYKcSV-LCoeQqfX1RYOo3ig4vwmRduz8A.woff2",
        "libs/6xKydSBYKcSV-LCoeQqfX1RYOo3ig4vwmBduz8A.woff2",
        "libs/6xKydSBYKcSV-LCoeQqfX1RYOo3ig4vwlBduz8A.woff2"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "world": "MAIN",
      "js": [
        "./js/document.js"
      ],
      "matches": [
        "https://*.youtube.com/*",
        "https://www.youtube-nocookie.com/embed/*"
      ],
      "exclude_matches": [
        "https://accounts.youtube.com/RotateCookiesPage*"
      ],
      "all_frames": true,
      "run_at": "document_start"
    },
    {
      "world": "ISOLATED",
      "js": [
        "./js/content.js"
      ],
      "css": [
        "content.css",
        "shared.css"
      ],
      "matches": [
        "https://*.youtube.com/*",
        "https://www.youtube-nocookie.com/embed/*"
      ],
      "exclude_matches": [
        "https://accounts.youtube.com/RotateCookiesPage*"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_title": "Serenity",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/IconSerenity16px.png",
      "32": "icons/IconSerenity32px.png",
      "64": "icons/IconSerenity64px.png",
      "128": "icons/IconSerenity128px.png"
    },
    "theme_icons": [
      {
        "light": "icons/IconSerenity16px.png",
        "dark": "icons/IconSerenity16px.png",
        "size": 16
      },
      {
        "light": "icons/IconSerenity32px.png",
        "dark": "icons/IconSerenity32px.png",
        "size": 32
      },
      {
        "light": "icons/IconSerenity64px.png",
        "dark": "icons/IconSerenity64px.png",
        "size": 64
      },
      {
        "light": "icons/IconSerenity128px.png",
        "dark": "icons/IconSerenity128px.png",
        "size": 128
      },
      {
        "light": "icons/IconSerenity256px.png",
        "dark": "icons/IconSerenity256px.png",
        "size": 256
      },
      {
        "light": "icons/IconSerenity512px.png",
        "dark": "icons/IconSerenity512px.png",
        "size": 512
      },
      {
        "light": "icons/IconSerenity1024px.png",
        "dark": "icons/IconSerenity1024px.png",
        "size": 1024
      }
    ]
  },
  "background": {
    "service_worker": "./js/background.js"
  },
  "manifest_version": 3
}